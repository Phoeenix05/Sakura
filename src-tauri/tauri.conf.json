{
  "build": {
    "beforeBuildCommand": "pnpm build:solid",
    "beforeDevCommand": "pnpm dev:solid",
    "devPath": "http://localhost:3000",
    "distDir": "../dist/public"
  },
  "package": {
    "productName": "mangadex-reader",
    "version": "0.0.1-dev"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "scope": [
          "$RESOURCES/*"
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "io.github.phoeenix05.mangadex-reader",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "mangadex-reader",
        "width": 800
      }
    ]
  }
}